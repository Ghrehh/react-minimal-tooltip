{
  "name": "react-minimal-tooltip",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "exports": {
    ".": {
      "import": "./dist/esm.js",
      "require": "./dist/cjs.js"
    }
  },
  "version": "0.1.34",
  "dependencies": {},
  "description": "Minimal React Tooltip Library",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "start": "rm -rf dist && node_modules/.bin/webpack-cli --mode development --watch --config webpack.config.js",
    "build": "rm -rf dist && npx webpack-cli --mode production",
    "publish": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghrehh/react-minimal-tooltip"
  },
  "keywords": [
    "react",
    "component",
    "tooltip",
    "tool-tip",
    "tool",
    "tip"
  ],
  "files": [
    "./dist/*"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "rollup": "^2.74.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}
